# Environment variables configuration

# Database URL (PostgreSQL)
DATABASE_URL=postgresql://localhost:5432/talktodata

# Or use SQLite for simpler setup (uncomment and comment PostgreSQL above)
# DATABASE_URL=sqlite:///./talktodata.db

# Google Gemini API Key
GEMINI_API_KEY=your_gemini_api_key_here

# File Upload Settings
UPLOAD_DIR=./uploads
MAX_FILE_SIZE_MB=100

# Application Settings
DEBUG=true
